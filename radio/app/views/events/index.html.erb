<h1>Listing Events</h1>

<table>
  <tr>
    <th>Artists</th>
    <th>Venue</th>
    <th>Date</th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.artists.collect{|a| a.name}.join(', ') %></td>
    <td><%= event.venue.name %></td>
    <td><%= event.start_date %></td>
  </tr>
<% end %>
</table>
